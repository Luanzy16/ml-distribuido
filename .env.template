# Unique identifier for the node
NODE_ID=node1

# Port the FastAPI server listens on (keep 8000)
PORT=8000

# Comma separated list of peers using private-ip:port (no spaces)
# Example: 10.0.1.10:8000,10.0.2.10:8000
PEERS=

# Replication factor for KV (how many peers to push to)
REPLICATE_TO=2

# Gossip interval seconds
GOSSIP_INTERVAL=3
